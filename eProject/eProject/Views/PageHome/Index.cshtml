<!-- Testimonials -->
<div class="slider-2">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">

                <!-- Text Slider -->
                <div class="slider-container">
                    <div class="swiper-container text-slider">
                        <div class="swiper-wrapper">

                            <!-- Slide -->
                            @foreach (var item in ViewBag.Review)
                            {
                                <div class="swiper-slide">

                                    <div class="image-wrapper">
                                        <img class="img-fluid" src="images/iconuser.png" alt="">
                                    </div> <!-- end of image-wrapper -->


                                    <div class="text-wrapper">
                                        <div class="testimonial-text">
                                            <p><i>"@Html.Raw(item.Comment.Replace("\n", "<br />"))"</i></p>
                                            @for (var i = 1; i <= item.Star; i++)
                                            {
                                                <span class="starGlowN"></span>
                                            }
                                            @for (var i = (item.Star + 1); i <= 10; i++)
                                            {
                                                <span class="starFadeN"></span>
                                            }
                                        </div>
                                        @foreach (var user in ViewBag.User)
                                        {
                                            if (user.UserId == item.ReviewerId)
                                            {
                                                <div class="testimonial-author">
                                                    @user.Username
                                                </div>
                                            }
                                        }
                                    </div> <!--end of text-wrapper-->


                                </div> <!-- end of swiper-slide -->
                            }
                            <!-- end of slide -->
                        </div> <!-- end of swiper-wrapper -->
                        <!-- Add Arrows -->
                        <div class="swiper-button-next"></div>
                        <div class="swiper-button-prev"></div>
                        <!-- end of add arrows -->

                    </div> <!-- end of swiper-container -->
                </div> <!-- end of slider-container -->
                <!-- end of text slider -->

            </div> <!-- end of col -->
        </div> <!-- end of row -->
