<!-- Customers -->
<div class="slider-1">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h2 class="h2-heading">End Auctions</h2>
            </div> <!-- end of col -->
        </div> <!-- end of row -->
        <div class="row">
            <div class="col-lg-12">

                <!-- Image Slider -->
                <div class="slider-container">
                    <div class="swiper-container image-slider">
                        <div class="swiper-wrapper">
                            @foreach (var item in ViewBag.End)

                            {
                                var images = item.Auction.Image.Split(",");
                                <div class="swiper-slide col-md-3">

                                    <div class="card card-ecommerce">

                                        <!--Card image-->
                                        <div class="view overlay">
                                            @{var image = item.Auction.Image.Split(",");}

                                            <img src="~/images/@image[0]" class="img-fluid" height="100">

                                            <a>
                                                <div class="mask rgba-white-slight"></div>
                                            </a>
                                        </div>
                                        <!--Card image-->
                                        <!--Card content-->
                                        <div class="card-body " style="text-align:left;">
                                            <!--Category & Title-->

                                            <h6 class="card-title mb-1">
                                                <strong>
                                                    @if (item.Auction.Title.Length < 20)
                                                    {
                                                        <a href="" class="dark-grey-text">@item.Auction.Title</a>
                                                    }
                                                    else
                                                    {
                                                        <a href="" class="dark-grey-text">@item.Auction.Title.Remove(20)</a>
                                                    }

                                                </strong>
                                            </h6>
                                            <span class="badge badge-danger mb-2">bestseller</span>

                                            <!--Card footer-->
                                            <div class="card-footer pb-0">
                                                <div class="row mb-0">
                                                    <span class="float-left">
                                                        <strong>@item.Auction.SalePrice $</strong>
                                                    </span>
                                                    <span class="float-right">

                                                        <a class="" data-toggle="tooltip" data-placement="top" title="Login to Bid">
                                                            <i class="fa fa-gavel ml-3"></i>
                                                        </a>
                                                    </span>
                                                </div>
                                            </div>

                                        </div>
                                        <!--Card content-->

                                    </div>
                                </div>
                            }
                        </div> <!-- end of swiper-wrapper -->
                    </div> <!-- end of swiper container -->
                </div> <!-- end of slider-container -->
                <!-- end of image slider -->

            </div> <!-- end of col -->
        </div> <!-- end of row -->
    </div> <!-- end of container -->
</div> <!-- end of slider-1 -->
<!-- end of customers -->
